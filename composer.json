{
	"name": "shredio/doctrine-queries",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.3",
		"doctrine/orm": "^3.2"
	},
	"autoload": {
		"psr-4": {
			"Shredio\\DoctrineQueries\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse --memory-limit=-1",
		"test": "vendor/bin/phpunit tests"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"phpunit/phpunit": "^12.2",
		"phpstan/phpstan-doctrine": "^2.0",
		"doctrine/orm": "^3.4",
		"symfony/cache": "^7.3",
		"symfony/clock": "^7.3"
	}
}
